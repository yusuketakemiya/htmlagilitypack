var oncekiBut = 1;
function bttn_over(myBut)	{
document.getElementById('m'+myBut).className = 'manset_bttn_over';
document.getElementById('manset'+myBut).style.display="";
document.getElementById('m'+oncekiBut).className = 'manset_bttn';
document.getElementById('manset'+oncekiBut).style.display="none";
oncekiBut=myBut;
}
var oncekiDeger = 1;
function td_over(i)	{
document.getElementById('spnsr'+oncekiDeger).style.display="none";
document.getElementById('spnsr'+i).style.display="";
document.getElementById('tdDay'+oncekiDeger).className='babylon_tab_bg';
document.getElementById('tdDay'+i).className='babylon_tab_bg_over';
oncekiDeger=i;
}
function td_click(myVal)
{
location.href = document.getElementById('spnsr'+myVal).getElementsByTagName("a")[0].href;
}
var oncekiManset = 1;	
function mansetAyarla(yeniManset){				
document.getElementById('m'+oncekiManset).className = 'manset_bttn';		
document.getElementById('m'+yeniManset).className = 'manset_bttn_over';									

document.getElementById('manset'+oncekiManset).style.display ='none';	
document.getElementById('manset'+yeniManset).style.display='';	
oncekiManset = yeniManset;
}
function linkAl(myValue)
{
location.href = document.getElementById('manset'+myValue).getElementsByTagName("a")[0].href;
}


function kitapcd_over(i)	{
switch(i)
{
case 'kitap':
document.getElementById('cd_div').style.display="none";
document.getElementById('kitap_div').style.display="";
document.getElementById('td_kitap').className='kitapcd_tab_bg_over';
document.getElementById('td_cd').className='kitapcd_tab_bg';
break
case 'cd':
document.getElementById('cd_div').style.display="";
document.getElementById('kitap_div').style.display="none";
document.getElementById('td_cd').className='kitapcd_tab_bg_over';
document.getElementById('td_kitap').className='kitapcd_tab_bg';
break
}
}

